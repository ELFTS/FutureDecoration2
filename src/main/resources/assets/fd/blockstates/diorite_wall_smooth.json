{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "fd:diorite_wall_smooth_post" }
    },
    {   "when": { "north": "true" },
      "apply": { "model": "fd:diorite_wall_smooth_side", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "fd:diorite_wall_smooth_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "true" },
      "apply": { "model": "fd:diorite_wall_smooth_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "fd:diorite_wall_smooth_side", "y": 270, "uvlock": true }
    }
  ]
}